@page
@model Vicevært.Web.Pages.Lejemaal.IndexModel


<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">


  </head>


   <div class="text-center" style="color:black">
                <br />
    <h2>Lejemål</h2>
    <p>Liste over Uniks lejemål</p>
</div>


  <body>
 <section class="container-fluid" id="first" style="color: black">   



    <article>
        <table class="table">
            <thead>
            <tr>
                <th>
                    <label asp-for="Lejemaal.First().LejemålId"></label>
                </th>

                <th>
                    <label>Gadenavn</label>
                </th>
                <th>
                    <label>Nummer</label>
                </th>
                <th>
                    <label>Andet nummer</label>
                </th>
                <th>
                    <label>Postnummer</label>
                </th>
                <th>
                    <label>By</label>
                </th>
                <th>
                    <label>Region</label>
                </th>
                <th>
                    <label>Land</label>
                </th>
                <th>
                    <label>Ledig</label>
                </th>
            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model.Lejemaal)
            {
                <tr>
                    <td>
                        @item.LejemålId
                    </td>
                    <td>
                        @item.StreetName
                    </td>
                    <td>
                        @item.BuildingNumber
                    </td>
                    <td>
                        @item.SecondaryAddress
                    </td>
                    <td>
                        @item.Postcode
                    </td>
                    <td>
                        @item.City
                    </td>
                    <td>
                        @item.State
                    </td>
                    <td>
                        @item.CountryCode
                    </td>
                    <td>
                        @item.IsBookable
                    </td>
                    <td>
                        <a asp-page="./Edit" asp-route-id="@item.LejemålId">Edit</a> |
                        <a asp-page="./Details" asp-route-id="@item.LejemålId">Details</a> |
                        <a asp-page="./Delete" asp-route-id="@item.LejemålId">Delete</a>
                    </td>
                </tr>
            }










 







            </tbody>
        </table>
    </article>

</section>

  </body>
</html>




